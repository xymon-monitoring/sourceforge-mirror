#include "windows.h"
#include "winver.h"

#include "version.h"

/////////////////////////////////////////////////////////////////////////////
//
// Version
//

/////////////////////////////////////////////////////////////////////////////


1 VERSIONINFO
 FILEVERSION BBWINVERSION
 PRODUCTVERSION BBWINVERSION
 FILEFLAGSMASK 0x3fL
#ifdef _DEBUG
 FILEFLAGS 0xbL
#else
 FILEFLAGS 0xaL
#endif
 FILEOS 0x10001L
 FILETYPE 0x1L
 FILESUBTYPE 0x0L
 BEGIN
    BLOCK "StringFileInfo"
      BEGIN
        BLOCK "040904E4"
          BEGIN
            VALUE "CompanyName", COMPANY_NAME
            VALUE "FileDescription", "stats agent : report general stats\0"
            VALUE "FileVersion", BBWINFILEVERSION_STR
            VALUE "LegalCopyright", BBWIN_COPYRIGHT
            VALUE "ProductName", BBWINNAME_STR
            VALUE "ProductVersion", BBWINVERSION_STR
			VALUE "Build", BBWINBUILD_STR
			VALUE "URL", BBWINURL_STR
			VALUE "Date", BBWINDATE_STR
          END
       END
    BLOCK "VarFileInfo"
      BEGIN
            VALUE "Translation", 0x409, 0x04E4
      END
 END

