all: bbgen bbtest larrdgen

GENOBJS = bbgen.o debug.o loaddata.o pagegen.o process.o util.o 
TESTOBJS = bbtest.o debug.o loaddata.o util.o
LARRDOBJS = larrdgen.o debug.o loaddata.o util.o

bbgen: $(GENOBJS)
	gcc -g -o bbgen $(GENOBJS)

bbtest: $(TESTOBJS)
	gcc -g -o bbtest $(TESTOBJS)

larrdgen: $(LARRDOBJS)
	gcc -g -o larrdgen $(LARRDOBJS)

%.o: %.c
	gcc -Wall -c -g -o $@ $<

clean:
	rm -f $(GENOBJS) $(TESTOBJS) $(LARRDOBJS) bbgen bbtest larrdgen
